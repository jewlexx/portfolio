{
  "name": "portfolio",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "stars"
  ],
  "scripts": {
    "astro": "astro",
    "build": "bun run build:wasm && astro build",
    "build:ci": "ci.sh",
    "build:wasm": "wasm-pack build ./stars --no-pack -Z build-std-features=panic_immediate_abort",
    "dev": "astro dev",
    "format": "prettier --write src/**/*",
    "postinstall": "bun run build:wasm",
    "preview": "astro build --config astro.preview.config.mjs && astro preview --config astro.preview.config.mjs",
    "start": "astro dev",
    "sync": "astro sync"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.0.1",
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.5",
    "@astrojs/solid-js": "^4.2.0",
    "@astrojs/svelte": "^5.4.0",
    "@fontsource/ibm-plex-mono": "^5.0.13",
    "@playform/compress": "^0.0.7",
    "@tabler/icons": "^3.5.0",
    "@tabler/icons-svelte": "3.5.0",
    "@vercel/speed-insights": "^1.0.11",
    "astro": "^4.9.1",
    "nanostores": "^0.10.3",
    "open-graph-scraper": "^6.5.1",
    "simple-get": "^4.0.1",
    "solid-js": "^1.8.17",
    "stars": "workspace:*",
    "svelte": "^4.2.17"
  },
  "devDependencies": {
    "@astrojs/node": "^8.2.5",
    "@astrojs/vercel": "^7.6.0",
    "@playform/document": "^0.0.7",
    "@playwright/test": "1.44.1",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vite-pwa/astro": "^0.4.0",
    "astro-compress": "^2.2.25",
    "astro-meta-tags": "0.3.0",
    "astro-robots-txt": "^1.0.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.1",
    "cypress": "^13.10.0",
    "eslint": "9.3.0",
    "eslint-config-prettier": "9.1.0",
    "postcss": "^8.4.38",
    "postcss-preset-env": "^9.5.14",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "^0.14.0",
    "prettier-plugin-svelte": "^3.2.3",
    "sass": "1.77.2",
    "sharp": "0.33.4",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "vite-plugin-wasm": "^3.3.0",
    "wasm-pack": "0.12.1"
  },
  "pnpm": {
    "overrides": {
      "file-type@^9": "^10",
      "typescript-esbuild": "^0.3"
    }
  },
  "trustedDependencies": [
    "@vercel/speed-insights"
  ]
}
