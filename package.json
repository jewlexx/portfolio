{
  "name": "jewlexx.github.io",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npx cross-env NODE_ENV=production next build",
    "deploy": "next export && node scripts/deploy.js",
    "dev": "next dev",
    "format": "prettier --write --ignore-path=.gitignore .",
    "format:check": "prettier --check --ignore-path=.gitignore .",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "next start",
    "test": "echo No tests found."
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "canvas-confetti": "1.5.1",
    "framer-motion": "6.2.9",
    "next": "12.1.4",
    "next-seo": "5.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/canvas-confetti": "1.4.2",
    "@types/eslint": "8.4.1",
    "@types/node": "^14",
    "@types/prettier": "2.6.0",
    "@types/react": "18.0.1",
    "@types/react-dom": "18.0.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "eslint-config-prettier": "8.5.0",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "sass": "1.50.0",
    "typescript": "4.6.3"
  }
}
